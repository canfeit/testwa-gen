{
  "name": "testwa-gen",
  "private": true,
  "version": "1.1.2",
  "main": "main/index.js",
  "description": "Automated test Script recording tool for Apps.",
  "author": "tanghongxin <hongxin.tang@hotmail.com> (http://cloud.testwa.com/)",
  "license": "MIT",
  "keywords": [
    "appium",
    "automation",
    "selenium",
    "testing",
    "testwa",
    "testwa-gen",
    "webdriver"
  ],
  "tags": [
    "android",
    "appium",
    "automation",
    "electron",
    "javascript",
    "node",
    "selenium",
    "testing",
    "testwa",
    "testwa-gen",
    "webdriver"
  ],
  "homepage": "https://github.com/tanghongxin/testwa-gen",
  "esm": {
    "cache": false
  },
  "scripts": {
    "build:dir": "yarn build:umi && yarn build:electron --dir -c.asar=true && rm -rf dist/renderer",
    "build:pack": "rm -rf dist && yarn build:electron",
    "build:electron": "build",
    "build:umi": "rm -rf dist && cd src/renderer && umi build",
    "clean": "yarn clean:bin && yarn clean:apk",
    "clean:apk": "adb uninstall io.appium.uiautomator2.server",
    "clean:bin": "adb shell rm -r /data/local/tmp/mini* && adb shell ls /data/local/tmp",
    "lint": "eslint . --ext .js",
    "lint:fix": "yarn lint --fix",
    "postinstall": "build install-app-deps",
    "release": "release-it -n && TODO",
    "dev": "concurrently --kill-others \"yarn dev:umi\" \"wait-on http://localhost:8000 --interval 50 && yarn dev:electron\"",
    "dev:umi": "cd src/renderer && umi dev",
    "dev:electron": "cd src && nodemon --watch main --exec electron main/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanghongxin/testwa-gen"
  },
  "bugs": {
    "url": "https://github.com/tanghongxin/testwa-gen/issues"
  },
  "dependencies": {
    "adbkit": "^2.11.0",
    "adbkit-apkreader": "^3.1.1",
    "ansi-to-html": "^0.6.9",
    "await-timeout": "^1.1.1",
    "axios": "^0.19.2",
    "electron-updater": "^4.0.6",
    "esm": "^3.2.25",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.15",
    "minicap": "^0.2.1-0",
    "minicap-prebuilt": "^2.3.0",
    "minitouch-prebuilt": "^1.2.0",
    "pouchdb-adapter-idb": "^7.0.0",
    "react-syntax-highlighter": "^10.1.2",
    "request": "^2.88.0",
    "rxdb": "^8.0.4",
    "rxjs": "^6.3.3",
    "wd": "^1.12.1",
    "xmldom": "^0.1.27",
    "xpath": "^0.0.27"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "15",
    "electron-builder": "^20.38.4",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^7.6.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "nodemon": "^2.0.2",
    "release": "^5.0.3",
    "release-it": "^10.1.0",
    "standard-version": "^4.4.0",
    "umi": "2.13.3",
    "umi-plugin-react": "^1.15.8",
    "wait-on": "^3.2.0"
  }
}
